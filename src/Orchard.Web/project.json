{
  "webroot": "wwwroot",
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-20704",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-20704",
    "Microsoft.NETCore.App": {
      "version": "1.0.0-rc2-3002426",
      "type": "platform"
    },
    "Orchard.Hosting.Console": "2.0.0",
    "Orchard.Hosting.Web": "2.0.0",
    "Orchard.Core": "2.0.0",
    "Orchard.Contents": "1.0.0",
    "Orchard.ContentFields": "1.0.0",
    "Orchard.ContentTypes": "1.0.0",
    "Orchard.Demo": "1.0.0",
    "Orchard.DynamicCache": "1.0.0",
    "Orchard.Lists": "1.0.0",
    "Orchard.Logging.Console": "2.0.0",
    "Orchard.Resources": "1.0.0",
    "Orchard.Setup": "2.0.0",
    "Orchard.Themes": "1.0.0",
    "SafeMode": "1.0.0",
    "TheAdmin": "1.0.0",
    "TheTheme": "1.0.0"
  },
  "compilationOptions": {
    "emitEntryPoint": true,
    "debugType": "portable",
    "preserveCompilationContext": true
  },
  "frameworks": {
    "netstandardapp1.5": {
      "dependencies": {
        "NETStandard.Library": "1.5.0-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-rc2-20704",
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "content": [
    "Views",
    "web.config"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "App_Data",
    "Core",
    "Modules",
    "Themes"
  ],
  "packExclude": [
    "node_modules",
    "bower_components",
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OrchardCMS/Orchard2"
  },
  "scripts": {
    "prepublish": [ "gulp publish" ],
    "postrestore": [ "gulp publish" ]
  }
}